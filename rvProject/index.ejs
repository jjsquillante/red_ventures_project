<% dealers.forEach((item, i) => { %>        
<div class="card">
  <div class="card--body">
  <% if(item.data.name.length > 25) { %>
    <div class="card--title card--content">
     <h1 class="font--is-large"><%= item.data.name %></h1>
    </div>                          
  <% } else { %>
    <div class="card--title">
	   <h1 class="font--is-large"><%= item.data.name %></h1>
    </div>
 <% } %>
